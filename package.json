{
  "name": "spacex-api",
  "version": "4.0.0",
  "description": "Open Source REST API for data about SpaceX",
  "exports": "./server.js",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run check-dependencies && jest --silent --verbose",
    "start": "node server.js",
    "worker": "node jobs/worker.js",
    "lint": "eslint .",
    "format": "prettier --write **/*.js",
    "check-dependencies": "npx depcheck --ignores=\"pino-pretty\",\"@jest/types\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-spacex/SpaceX-API"
  },
  "keywords": [
    "spacex",
    "space",
    "rocket",
    "rest-api",
    "mongodb",
    "koa"
  ],
  "author": "Jake Meyer <jakewmeyer@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/r-spacex/SpaceX-API/issues"
  },
  "homepage": "https://github.com/r-spacex/SpaceX-API",
  "dependencies": {
    "blake3": "^2.1.7",
    "cheerio": "^1.0.0-rc.10",
    "cron": "^1.8.2",
    "fastify": "^3.25.2",
    "fuzzball": "^2.1.2",
    "got": "^12.0.0",
    "ioredis": "^4.28.2",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-conditional-get": "^3.0.0",
    "koa-etag": "^4.0.0",
    "koa-helmet": "^6.1.0",
    "koa-pino-logger": "^3.0.0",
    "koa-router": "^10.1.1",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.21",
    "moment-range": "^4.0.2",
    "moment-timezone": "^0.5.34",
    "mongoose": "^6.1.3",
    "mongoose-id": "^0.1.3",
    "mongoose-paginate-v2": "^1.4.2",
    "pino": "^7.6.1",
    "rss-parser": "^3.12.0",
    "tle.js": "^4.4.0",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-mongodb": "^1.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "pino-pretty": "^7.3.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
